trim.xy <- function(xy,trim = 0.2){
  xy <- as.matrix(xy)
  xy <- xy[ !is.na(xy[,1]) & !is.na(xy[,2]), ]
  xlimits <- quantile(xy[,1], c(trim, 1-trim))
  ylimits <- quantile(xy[,2], c(trim, 1-trim))
  xy <- xy[ xlimits[1] <= xy[,1] & xlimits[2] >= xy[,1] &
            ylimits[1] <= xy[,2] & ylimits[2] >= xy[,2] ,]
  return(xy)
}
